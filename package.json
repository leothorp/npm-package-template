{
  "name": "@leothorp/npm-package-template",
  "version": "0.0.26",
  "description": "",
  "type": "module",
  "scripts": {
    "build": "babel src -d dist",
    "test": "node tests/run.js",
    "repub-and-test": "bash -c 'source ~/.bash_profile && npm-pub test && sleep 2 && npm run test'"
  },
  "engines": {
    "node": "^14 || ^16 || >=18"
  },
  "exports": {
    ".": {
      "browser": "./index.browser.js",
      "default": "./index.js"
    }
  },
  "browser": {
    "./index.js": "./index.browser.js"
  },
  "keywords": [],
  "author": "leothorp",
  "repo": "https://github.com/leothorp/npm-package-template",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6"
  },
  "dependencies": {
    "nanoid": "^4.0.0",
    "shelljs": "^0.8.5"
  },
  "files": [
    "src/*",
    "bin/*",
    "index.js",
    "index.browser.js",
    "README.md"
  ],
  "bin": {
    "@leothorp/npm-package-template": "./bin/npm-package-template.js"
  }
}
